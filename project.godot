; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="2DNonname"
run/main_scene="res://scenes/main_scene.tscn"
config/icon="res://Textures/icon.png"

[autoload]

MainCharacter="*res://Scripts/GlobalScripts/MainCharacter.gd"
WindowsInfo="*res://Scripts/GlobalScripts/WindowsInfo.gd"
JsonManager="*res://Scripts/GlobalScripts/JsonManager.gd"
SceneManager="*res://Scripts/GlobalScripts/SceneManager.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
window/size/test_width=1200
window/size/test_height=700
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

environment/default_environment="res://default_env.tres"
